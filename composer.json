{
    "name": "abeatrice/logparser",
    "description": "Parse access log files for geo location information",
    "type": "library",
    "authors": [
        {
            "name": "Andrew Beatrice"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "symfony/console": "~4.0",
        "php": ">=8.0",
        "symfony/filesystem": "^5.2",
        "kassner/log-parser": "~2.0",
        "geoip2/geoip2": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-bin-dir": "bin"
    }
}
